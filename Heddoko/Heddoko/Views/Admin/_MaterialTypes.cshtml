<div class="row">
    <div class="col-md-4">
        <div class="filters materialTypesFilter">
            <div>
                <label for="searchMaterialType" class="control-label">@i18n.Resources.Search</label>
                <input id="searchMaterialType"
                       class="form-control"
                       type="text"
                       placeholder="@i18n.Resources.EnterIdentifier"
                       data-bind="value: search, events:{ keyup: keyup }" />
            </div>
            <div class="margin010">
                <button type="button" data-bind="click: find" class="btn btn-default">@i18n.Resources.Find</button>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="panel panel-success materialTypesForm">
            <div class="panel-heading clearfix">
                <h3 class="panel-title pull-left">@i18n.Resources.AddANew @i18n.Resources.MaterialType</h3>
                <div class="btn-group pull-right" role="group">
                    <button type="button" class="btn btn-sm btn-warning" data-bind="click: reset">@i18n.Resources.Reset</button>
                    <button type="button" class="btn btn-sm btn-success" data-bind="click: submit">@i18n.Resources.Submit</button>
                </div>
            </div>
            <div class="panel-body">
                <div class="row">
                    <div class="col-sm-12">
                        <input type="text"
                               required
                               name="materialType-identifier"
                               validationMessage="@i18n.Resources.Required"
                               placeholder="@i18n.Resources.Identifier"
                               class="form-control"
                               data-bind="value: model.identifier" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div id="gridMaterialTypes">

        </div>
    </div>
</div>
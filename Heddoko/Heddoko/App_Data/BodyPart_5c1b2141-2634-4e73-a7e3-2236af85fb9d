using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace HeddokoSDK
{
    public class HeddokoConfig
    {
        public string APIUrl { get; set; }
        public static string JWTSecret { get; set; }

        public HeddokoConfig(string apiURL, string jwtSecret)
        {
            APIUrl = apiURL;
            JWTSecret = jwtSecret;
        }
    }
}
